using System;
using System.Collections.Generic;

public class Program
{
    public static void SelectionSort(List<int> arr)
    {
        int n = arr.Count;
        for (int i = 0; i < n - 1; i++) // loop all the array value
        {
            int minIndex = i;
            for (int j = i + 1; j < n; j++)
            {
                if (arr[j] < arr[minIndex])
                {
                    minIndex = j; // find the minimum integers
                }
            }
            int temp = arr[minIndex]; // swap the min integer with the 1st integer
            arr[minIndex] = arr[i];
            arr[i] = temp;
        }
    }

    public static void Main()
    {
        List<int> numbers = new List<int> { 45, 22, 67, 34, 100, 49, 119, 98, 37, 11 }; // list of the integers
        SelectionSort(numbers); // call the sort function

        Console.WriteLine("Sorted list:");
        foreach (int num in numbers)
        {
            Console.Write(num + " ");
        }
    }
}


the output : 
Sorted list:
11 22 34 37 45 49 67 98 100 119
